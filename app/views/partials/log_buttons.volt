<div class="row p-2">
    <div class="col">
        <button class="btn btn-primary btn-sm" onclick="log_start('{{ id }}', '{{ day }}', {{ isAdmin }});">Start</button>
        <button class="btn btn-danger btn-sm" onclick="log_stop('{{ id }}', '{{ day }}', {{ isAdmin }});">Stop</button>
    </div>
</div>